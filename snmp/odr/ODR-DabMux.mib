-- mux Info
----------------------------------
muxVersion OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS read-only
        STATUS  current
        DESCRIPTION
                "ODR-DabMux version"
        -- 1.3.6.1.4.1.51436.1.x.1
        ::= { dabmux 1 }

muxGlobalInputState OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS read-only
        STATUS  current
        DESCRIPTION
                "ODR-DabMux global input state"
        -- 1.3.6.1.4.1.51436.1.x.2
        ::= { dabmux 2 }
        
----------------------------------
-- inputTable 
----------------------------------
  
inputTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF inputEntrySEQ
        MAX-ACCESS not-accessible
        STATUS  current
        DESCRIPTION
                ""
        -- 1.3.6.1.4.1.51436.1.x.10
        ::= { dabmux 10 }

inputEntry OBJECT-TYPE
        SYNTAX  inputEntrySEQ
        MAX-ACCESS not-accessible
        STATUS  current
        DESCRIPTION
                ""
        INDEX {
                inputIndex }
        -- 1.3.6.1.4.1.51436.1.x.10.1
        ::= { inputTable 1 }


